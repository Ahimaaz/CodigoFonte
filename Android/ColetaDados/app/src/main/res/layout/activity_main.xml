<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.eric.coletadedados.MainActivity">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp">

        <Button
            android:id="@+id/btnIniciar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Iniciar"
            tools:layout_editor_absoluteX="62dp"
            tools:layout_editor_absoluteY="55dp"
            android:layout_marginTop="69dp"
            android:layout_marginLeft="68dp"
            android:layout_marginStart="68dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/btnParar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Parar"
            tools:layout_editor_absoluteX="220dp"
            tools:layout_editor_absoluteY="55dp"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:layout_alignBaseline="@+id/btnIniciar"
            android:layout_alignBottom="@+id/btnIniciar"
            android:layout_toRightOf="@+id/btnIniciar"
            android:layout_toEndOf="@+id/btnIniciar" />

    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
